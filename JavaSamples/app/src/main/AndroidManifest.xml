<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.emedinaa.myfirstapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:label="JavaSamples">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity"
            android:label="Formulario"/>
        <activity android:name=".RegisterBaseActivity"
            android:label="Formulario Base"/>
        <activity android:name=".CalculadoraActivity"
            android:label="Calculadora"/>
        <activity android:name=".CalculadoraBaseActivity"
            android:label="Calculadora Base"/>
    </application>

</manifest>