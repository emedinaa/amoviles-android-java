<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.emedinaa.myfirstapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".StarWarsEventsActivity" />
        <activity android:name=".EventDetailsActivity" />
        <activity android:name=".StarWarsEventsFragmentActivity" />
        <activity
            android:name=".StarWarsEventsTabletActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".EventDetailFragmentActivity" />
        <activity
            android:name=".MainMessageActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".FragmentBasicActivity" />
        <activity android:name=".FragmentProgrammingActivity" />
        <activity android:name=".FragmentCommunicationActivity" />
    </application>

</manifest>