<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.emedinaa.myfirstapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <!--<intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>-->
        </activity>

        <activity
            android:name=".ProcessAsynctaskActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ProcessImageAsynctaskActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ProcessProgressAsynctaskActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".ProcessThreadsActivity" />
        <activity android:name=".MainIntentServiceActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".service.CustomIntentService" />
        <service
            android:name=".MyService"
            android:enabled="true" />
        <service
            android:name=".MyIntentService"
            android:enabled="true" />
    </application>

</manifest>